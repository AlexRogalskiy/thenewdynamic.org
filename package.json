{
  "name": "the-new-dynamic-org",
  "version": "3.0.0",
  "description": "The New Dynamic .org website",
  "main": "index.js",
  "repository": "https://github.com/thenewdynamic-org/thenewdynamic.org",
  "author": "budparr",
  "license": "MIT",
  "scripts": {
    "algolia": "atomic-algolia",
    "version:major": "npm version major -m 'Breaking Release'",
    "version:minor": "npm version minor -m 'Feature Release'",
    "version:patch": "npm version patch -m 'bump version for fixes minor changes'",
    "browsersync-public": "browser-sync start --server 'public' --files 'public' reload",
    "bs": "browser-sync start --config 'bs-config.js'",
    "browsersync-old": "browser-sync start --port 1313 --files reload 'content, src, themes' reload",
    "buildp": "webpack --config webpack.prod.js --env=production",
    "buildd": "webpack --config webpack.dev.js --env=development --progress --colors --watch",
    "hugo": "hugo server --disableFastRender --disableLiveReload",
    "hugoBuild": "hugo -w",
    "start": "concurrently \"npm run hugo\" \"npm run buildd\" \"npm run bs\" "
  },
  "devDependencies": {
    "atomic-algolia": "^0.3.11",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.18",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.9",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "glob-all": "^3.1.0",
    "instantsearch.js": "^2.5.1",
    "lazysizes": "^4.0.1",
    "list.js": "^1.5.0",
    "mustache-loader": "^1.4.0",
    "postcss": "^6.0.17",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.1.0",
    "purgecss-webpack-plugin": "^0.20.0",
    "style-loader": "^0.20.1",
    "tailwindcss": "^0.4.1",
    "turbolinks": "^5.1.1",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "npm": "^5.7.1",
    "to": "^0.2.9",
    "update": "^0.7.4"
  }
}
